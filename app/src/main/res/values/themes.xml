        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    </style>

    <style name="Text22">
        <item name="android:textSize">@dimen/Title_text_22</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
    </style>

    <style name="Text22_400">
        <item name="android:textSize">@dimen/Title_text_22</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textColor">@color/textColor_white</item>
    </style>

    <style name="Text19">
        <item name="android:textSize">@dimen/Search_Text_19</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">400</item>
    </style>

    <style name="Text16">
        <item name="android:textSize">@dimen/Settings_Text_16</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
    </style>

    <style name="Text14">
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
    </style>

    <style name="Text14_400">
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textColor">@color/textColor_white</item>
    </style>

    <style name="Text13_400">
        <item name="android:textSize">13sp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textColor">@color/switch_thumb_off_color</item>
    </style>

    <style name="Text13_400_">
        <item name="android:textSize">13sp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textColor">@color/textColor_white</item>
    </style>

    <style name="Text11">
        <item name="android:textSize">11sp</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>

    <style name="Title" parent="Text22">
    </style>

    <style name="SettingsTextView" parent="Text16">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/Settings_Height_61</item>
        <item name="android:textColor">@color/black_white</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingStart">@dimen/Padding_16</item>
        <item name="android:paddingEnd">@dimen/Padding_18</item>
    </style>

    <style name="SwitchView" parent="SettingsTextView">
        <item name="android:paddingEnd">@dimen/Switch_padding_6</item>
        <item name="android:layout_marginTop">@dimen/Separator_10</item>
    </style>

    <style name="SearchBox">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxHeight">@dimen/searchLineHeight_52</item>
        <item name="boxBackgroundMode">outline</item>
        <item name="hintEnabled">true</item>
        <item name="android:hint">@string/search_hint</item>
        <item name="android:textColorHint">@color/hintColor_textColor</item>
        <item name="boxStrokeWidth">@dimen/searchNoStroke</item>
        <item name="boxStrokeColor">@color/transparent</item>
        <item name="boxCornerRadiusTopStart">@dimen/searchRadius_8</item>
        <item name="boxCornerRadiusTopEnd">@dimen/searchRadius_8</item>
        <item name="boxCornerRadiusBottomStart">@dimen/searchRadius_8</item>
        <item name="boxCornerRadiusBottomEnd">@dimen/searchRadius_8</item>
        <item name="android:paddingHorizontal">@dimen/searchPaddingLeftRight_16</item>
        <item name="android:paddingVertical">@dimen/searchPaddingTopBottom_8</item>
        <item name="boxBackgroundColor">@color/hintField_white</item>
        <item name="cursorColor">@color/cursorColor</item>
    </style>

    <style name="SearchInput" parent="Text16">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxHeight">@dimen/searchBoxHeight_36</item>
        <item name="android:maxLines">1</item>
        <item name="android:inputType">text</item>
        <item name="android:textColor">@color/textColor</item>
        <item name="android:drawableStart">@drawable/search_icon</item>
        <item name="android:drawablePadding">8dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:drawableTint">@color/hintColor_textColor</item>
        <item name="android:paddingVertical">@dimen/searchIconAndHintVerticalPadding_8.5</item>
        <item name="android:maxLength">30</item>
    </style>

    <style name="ClearIcon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">end|center_vertical</item>
        <item name="android:contentDescription">@string/clear_search</item>
        <item name="android:src">@android:drawable/ic_menu_close_clear_cancel</item>
        <item name="android:scaleX">0.5</item>
        <item name="android:scaleY">0.5</item>
        <item name="android:padding">@dimen/searchEraserPadding_16</item>
        <item name="android:visibility">gone</item>
        <item name="tint">@color/hintColor_textColor</item>
    </style>

    <style name="SearchFail" parent="Text19">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">@dimen/SearchFailTop_102</item>
        <item name="android:drawablePadding">@dimen/Padding_16</item>
        <item name="android:text">@string/searchFail</item>
        <item name="android:textColor">@color/textColor_white</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="SearchUpdate" parent="Text14">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginTop">@dimen/arrow_back_24</item>
        <item name="android:text">@string/clean</item>
        <item name="android:textColor">@color/white_textColor</item>
        <item name="backgroundTint">@color/textColor_white</item>
        <item name="cornerRadius">@dimen/track_45</item>
        <item name="android:inputType">text</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="track_author" parent="Text11">
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/hintColor_white</item>
        <item name="drawableTint">@color/hintColor_white</item>
    </style>

    <style name="track_name" parent="Text16">
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/black_white</item>
    </style>

    <style name="arrow_fw">
        <item name="android:tint">@color/hintColor_white</item>
    </style>

    <style name="SearchHistoryTextView" parent="Text19">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/searchLineHeight_52</item>
        <item name="android:layout_marginTop">@dimen/arrow_back_24</item>
        <item name="android:layout_marginBottom">@dimen/searchPaddingTopBottom_8</item>
        <item name="android:textColor">@color/black_white</item>
        <item name="android:gravity">center</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:visibility">gone</item>

    </style>


